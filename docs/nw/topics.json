---
layout: null
permalink: /docs/nw/topics.json
---
[
{%- assign first = true -%}
{%- for p in site.pages -%}
  {%- assign url = p.url | default: "" -%}
  {%- if url contains "/docs/nw/" -%}
    {%- if url contains "/docs/nw/exam" -%}
      {%- continue -%}
    {%- endif -%}
    {%- if url == "/docs/nw/" -%}
      {%- continue -%}
    {%- endif -%}
    {%- if p.name == "index.md" -%}
      {%- continue -%}
    {%- endif -%}
    {%- unless first -%},{%- endunless -%}
    {%- assign first = false -%}
    {
      "title": {{ p.title | jsonify }},
      "url": {{ url | jsonify }},
      "parent": {{ p.parent | jsonify }},
      "grand_parent": {{ p.grand_parent | jsonify }},
      "nav_order": {{ p.nav_order | default: 0 }},
      "source_path": {{ p.path | jsonify }}
    }
  {%- endif -%}
{%- endfor -%}
]

